{
  "name": "kikobeats.com",
  "description": "",
  "version": "1.0.0",
  "author": "",
  "keywords": [],
  "devDependencies": {
    "acho": "latest",
    "browser-sync": "latest",
    "condense-whitespace": "latest",
    "gulp": "latest",
    "gulp-imagemin": "latest",
    "gulp-util": "latest",
    "husky": "latest",
    "imageoptim-cli": "latest",
    "lint-staged": "latest",
    "standard": "latest",
    "standard-markdown": "latest",
    "svgo": "latest",
    "tachyons-sass": "latest"
  },
  "engines": {
    "node": ">= 8"
  },
  "scripts": {
    "build": "NODE_ENV=production JEKYLL_ENV=production bundle jekyll build",
    "dev": "gulp",
    "lint": "standard-markdown && standard",
    "precommit": "lint-staged",
    "pretest": "npm run lint",
    "test": "exit 0"
  },
  "private": true,
  "license": "MIT",
  "lint-staged": {
    "package.json": [
      "finepack",
      "git add"
    ],
    "*.js": [
      "prettier-standard",
      "git add"
    ],
    "*.md": [
      "standard-markdown",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif}": [
      "imageoptim --imagealpha",
      "git add"
    ],
    "*.svg": [
      "svgo --pretty",
      "git add"
    ]
  },
  "urlint": {
    "url": "https://kikobeats.com/sitemap.xml",
    "whitelist": [
      "https://www.linkedin.com/in/kikobeats"
    ]
  }
}
